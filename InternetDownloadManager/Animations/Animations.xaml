<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Storyboard
        x:Key="AnimationurlTextBox"
        AutoReverse="True"
        RepeatBehavior="Forever">
        <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Effect).(DropShadowEffect.BlurRadius)">
            <LinearDoubleKeyFrame KeyTime="00:00:00:01" Value="21" />
            <LinearDoubleKeyFrame KeyTime="00:00:00:01.2" Value="22" />
            <LinearDoubleKeyFrame KeyTime="00:00:00:01.2" Value="23" />
            <LinearDoubleKeyFrame KeyTime="00:00:00:01.6" Value="24" />
        </DoubleAnimationUsingKeyFrames>
        <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Effect).(DropShadowEffect.Color)">
            <LinearColorKeyFrame KeyTime="00:00:00:01" Value="#DBDBDB" />
            <LinearColorKeyFrame KeyTime="00:00:00:01.2" Value="#CFCFCF" />
            <LinearColorKeyFrame KeyTime="00:00:00:01.2" Value="#C2C2C2" />
            <LinearColorKeyFrame KeyTime="00:00:00:01.6" Value="#B8B8B8" />
        </ColorAnimationUsingKeyFrames>
    </Storyboard>
</ResourceDictionary>